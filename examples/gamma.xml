    <var type="gamma">
     <param name="k"><m:cn>10</m:cn></param>
     <param name="theta"><m:cn>100</m:cn></param>
    </var>
    <var type="gamma">
     <param name="k"><m:cn>10</m:cn></param>
     <param name="theta"><m:cn>100</m:cn></param>
    </var>
    <var type="gamma">
     <param name="k"><m:cn>10</m:cn></param>
     <param name="theta"><m:cn>100</m:cn></param>
    </var>
   </var>
  </pdf>
  <pdf>
   <var type="gamma">
    <param name="k"><m:cn>30</m:cn></param>
    <param name="theta"><m:cn>100</m:cn></param>
   </var>
  </pdf>
 </output>
</simulation>
