<?xml version="1.0"?>

<module xmlns="http://hmatuschek.github.com/stochbb/module-0.0.dtd"
        xmlns:m="http://www.w3.org/1998/Math/MathML">

 <define name="chisq">
  <param id="nu"/>

  <var type="gamma" id="chisq">
   <param name="k">
    <m:apply>
     <m:divide/>
     <m:ci>nu</m:ci>
     <m:cn>2</m:cn>
    </m:apply>
   </param>
   <param name="theta">
    <m:cn>2</m:cn>
   </param>
  </var>

 </define>

</module>
