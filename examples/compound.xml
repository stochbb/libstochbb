<?xml version="1.0"?>

<simulation xmlns="http://hmatuschek.github.io/stochbb/simulation-0.0.dtd"
            xmlns:m="http://www.w3.org/1998/Math/MathML">

  <var type="normal" id="mu">
   <param name="mu"> <m:cn>100</m:cn> </param>
   <param name="sigma"> <m:cn>20</m:cn> </param>
  </var>

  <output from="-100" to="200" steps="100">
   <var type="chain" name="analytic">
    <var ref="mu"/>
    <var type="normal">
     <param name="mu"> <m:cn>0</m:cn> </param>
     <param name="sigma"> <m:cn>20</m:cn> </param>
    </var>
   </var>

   <var type="compound" base="normal" name="numeric">
    <param name="mu"> <var ref="mu"/> </param>
    <param name="sigma">
     <var type="delta">
      <param name="delay"> <m:cn>20</m:cn> </param>
     </var>
    </param>
   </var>
   <var ref="mu"/>
  </output>

</simulation>
