<?xml version="1.0"?>

<simulation xmlns="http://github.com/hmatuschek/stochbb/version-0">
 <!-- Get 3 iid. Gamma(3,10) distributed RV -->
 <var id="X1" type="gamma">
  <param name="k">5</param>
  <param name="theta">10</param>
 </var>
 <var id="X2" type="gamma">
  <param name="k">5</param>
  <param name="theta">10</param>
 </var>
 <var id="X3" type="gamma">
  <param name="k">5</param>
  <param name="theta">10</param>
 </var>

 <var id="min" type="minimum">
  <ref>X1</ref> <ref>X2</ref> <ref>X3</ref>
 </var>
 <var id="max" type="maximum">
  <ref>X1</ref> <ref>X2</ref> <ref>X3</ref>
 </var>

 <output from="0" to="50" steps="1000">
  <pdf><ref>X1</ref></pdf>
  <pdf><ref>min</ref></pdf>
  <pdf><ref>max</ref></pdf>
 </output>
</simulation>
