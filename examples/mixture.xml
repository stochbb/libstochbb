<?xml version="1.0"?>

<simulation xmlns="http://hmatuschek.github.io/stochbb/simulation-0.0.dtd"
            xmlns:m="http://www.w3.org/1998/Math/MathML">

 <param id="sigma"> <m:cn>3</m:cn> </param>

 <output from="0" to="40" steps="10000">
  <var type="mixture" id="mix">
   <weight>
    <m:cn> 1 </m:cn>
    <var type="normal">
     <param name="mu"> <m:cn>10</m:cn> </param>
     <param name="sigma"> <m:ci>sigma</m:ci> </param>
    </var>
   </weight>
   <weight>
    <m:cn> 2 </m:cn>
    <var type="normal">
     <param name="mu"> <m:cn>20</m:cn> </param>
     <param name="sigma"> <m:ci>sigma</m:ci> </param>
    </var>
   </weight>
   <weight>
    <m:cn> 1 </m:cn>
    <var type="normal">
     <param name="mu"> <m:cn>30</m:cn> </param>
     <param name="sigma"> <m:ci>sigma</m:ci> </param>
    </var>
   </weight>
  </var>  
 </output>

</simulation>
