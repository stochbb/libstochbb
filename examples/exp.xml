<?xml version="1.0"?>

<module xmlns="http://github.com/hmatuschek/stochbb/module-0.0"
        xmlns:m="http://www.w3.org/1998/Math/MathML">

 <define name="exp">
  <param id="lambda"/>

  <var type="gamma" id="exp">
   <param name="k"><m:cn>1</m:cn></param>
   <param name="theta">
    <m:apply>
     <m:divide/>
     <m:cn>1</m:cn>
     <m:ci>lambda</m:ci>
    </m:apply>
   </param>
  </var>
 </define>

</module>
